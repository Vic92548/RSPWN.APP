{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vapr",
  "version": "1.1.17",
  "identifier": "com.vapr.club",
  "build": {
    "devUrl": "http://localhost:8080",
    "frontendDist": "https://vapr.club"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "vapr.club",
        "width": 1600,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
